XF86MonBrightnessDown
  xbacklight -dec 5

XF86MonBrightnessUp
  xbacklight -inc 5

XF86Audio{Raise,Lower}Volume  
  pulsemixer --change-volume {+,-}1  
 
XF86AudioMute  
  pulsemixer  --toggle-mute

super + shift + w
  camtoggle

super + shift + p
  powermenu

super + shift + g
  notify-send "Appointments" "$(calcurse -d 3)"

Print
  flameshot gui

super + shift + n
  st -e nmtui

